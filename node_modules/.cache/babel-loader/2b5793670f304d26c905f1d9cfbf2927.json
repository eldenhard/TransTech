{"ast":null,"code":"import api from '@/api/directory';\nimport { mapState } from 'vuex';\nimport Loader from '@/components/loader/loader.vue';\nimport Notifications from '@/components/notifications/Notifications.vue';\nimport FilterProvider from '@/components/filter/contractFilter/filter_provider.vue';\nimport groups from '@/helpers/groups';\nexport default {\n  name: 'PartnerTable',\n  components: {\n    Loader,\n    Notifications,\n    FilterProvider\n  },\n\n  data() {\n    return {\n      nextLink: null,\n      prevLink: null,\n      loader: false,\n      total_objects: '',\n      amount: '',\n      providerDirectory: '',\n      // Уведомления\n      showNotify: false,\n      notifyHead: '',\n      notifyMessage: '',\n      notifyClass: '',\n      filter_provider: {\n        number: '',\n        counterparty__full_name: ''\n      }\n    };\n  },\n\n  methods: {\n    getGroupName(group) {\n      console.log(groups);\n      return groups.groups[group];\n    },\n\n    goToPage(link) {\n      let url = new URL(link);\n      let pageNumber = url.searchParams.get(\"page\");\n\n      if (pageNumber != null) {\n        this.filter_provider.page = pageNumber;\n      } else {\n        delete this.filter_provider.page;\n      }\n\n      this.getProviderContract();\n    },\n\n    getProviderContract() {\n      this.loader = true;\n      api.getDirectoryProvider(this.filter_provider).then(response => {\n        this.nextLink = response.data.links.next;\n        this.prevLink = response.data.links.previous;\n        this.providerDirectory = response.data.data;\n        this.total_objects = response.data.total_objects;\n        this.amount = response.data.amount;\n        this.notifyHead = 'Успешно';\n        this.notifyMessage = 'Данные отфильтрованы';\n        this.notifyClass = 'wrapper-success';\n        this.showNotify = true;\n        setTimeout(this.closeNotification, 1500);\n        this.loader = false;\n      }).catch(err => {\n        this.loader = false;\n        this.notifyHead = 'Ошибка';\n        this.notifyMessage = 'Данные не отфильтрованы, попробуйте еще раз';\n        this.notifyClass = 'wrapper-alert';\n        this.showNotify = true;\n        setTimeout(this.closeNotification, 1500);\n      });\n    },\n\n    closeNotification() {\n      this.showNotify = false;\n    },\n\n    updateFilterDataProvider(filter_provider) {\n      this.filter_provider = filter_provider;\n    }\n\n  },\n  computed: { ...mapState({\n      user: state => state.auth.user,\n      uid: state => state.auth.uid,\n      allGroups: state => state.auth.groups,\n      staffGlobal: state => state.auth.users\n    })\n  }\n};","map":{"version":3,"mappings":"AA4MA;AACA;AACA;AACA;AACA;AACA;AAEA;EACAA,oBADA;EAEAC;IAAAC;IAAAC;IAAAC;EAAA,CAFA;;EAGAC;IACA;MACAC,cADA;MAEAC,cAFA;MAGAC,aAHA;MAIAC,iBAJA;MAKAC,UALA;MAMAC,qBANA;MAQA;MACAC,iBATA;MAUAC,cAVA;MAWAC,iBAXA;MAYAC,eAZA;MAcAC;QACAC,UADA;QAEAC;MAFA;IAdA;EAqBA,CAzBA;;EA0BAC;IACAC;MACAC;MACA;IACA,CAJA;;IAKAC;MACA;MACA;;MACA;QACA;MACA,CAFA,MAEA;QACA;MACA;;MACA;IACA,CAdA;;IAeAC;MACA;MACAC,+CACAC,IADA,CACAC;QACA;QACA;QACA;QACA;QACA;QAEA;QACA;QACA;QACA;QACAC;QACA;MACA,CAdA,EAcAC,KAdA,CAcAC;QACA;QACA;QACA;QACA;QACA;QACAF;MACA,CArBA;IAuBA,CAxCA;;IA0CAG;MACA;IACA,CA5CA;;IA6CAC;MACA;IACA;;EA/CA,CA1BA;EA4EAC,YACA;MACAC,8BADA;MAEAC,4BAFA;MAGAC,qCAHA;MAIAC;IAJA;EADA;AA5EA","names":["name","components","Loader","Notifications","FilterProvider","data","nextLink","prevLink","loader","total_objects","amount","providerDirectory","showNotify","notifyHead","notifyMessage","notifyClass","filter_provider","number","counterparty__full_name","methods","getGroupName","console","goToPage","getProviderContract","api","then","response","setTimeout","catch","err","closeNotification","updateFilterDataProvider","computed","user","uid","allGroups","staffGlobal"],"sourceRoot":"src/components/Table/Contracts","sources":["providerContract.vue"],"sourcesContent":["<template>\r\n\r\n    <div>\r\n        <FilterProvider @updateFilterDataProvider=\"updateFilterDataProvider\"></FilterProvider>\r\n        <Notifications :show=\"showNotify\" :header=\"notifyHead\" :message=\"notifyMessage\" :block-class=\"notifyClass\"\r\n            id=\"notif\" />\r\n        <Loader :loader=\"loader\"></Loader>\r\n        <button class=\"Accept\" @click=\"getProviderContract()\"\r\n            style=\"width: 100%; position: relative; left: 50%; transform: translate(-50%,0)\">Запросить\r\n            договора с поставщиками</button>\r\n        <br><br>\r\n        <p class=\"amount\">всего записей: {{ total_objects }}</p>\r\n        <p class=\"amount\">всего на странице: {{ amount }}</p>\r\n        <div\r\n            style=\"width: 100%; overflow-x: auto; height: 80vh; overflow-y: auto; position: relative; left: 50%; transform: translate(-50%,0); margin-bottom: 3%;\">\r\n            <table class=\"table\" style=\"table-layout: fixed;\">\r\n                <thead>\r\n                    <tr>\r\n                        <th\r\n                            style=\"width:  200px !important; height: 50px !important; vertical-align: middle !important;\">\r\n                            Номер договора</th>\r\n                        <th\r\n                            style=\"width:  200px !important; height: 50px !important; vertical-align: middle !important;\">\r\n                            Статус ТТ по договору</th>\r\n                        <th\r\n                            style=\"width:  200px !important; height: 50px !important; vertical-align: middle !important;\">\r\n                            Дата заключения</th>\r\n                        <th\r\n                            style=\"width:  200px !important; height: 50px !important; vertical-align: middle !important;\">\r\n                            Подразделение инициатора</th>\r\n                        <th\r\n                            style=\"width:  200px !important; height: 50px !important; vertical-align: middle !important;\">\r\n                            Вид договора</th>\r\n                        <th\r\n                            style=\"width:  200px !important; height: 50px !important; vertical-align: middle !important;\">\r\n                            Предмет договора</th>\r\n                        <th\r\n                            style=\"width:  200px !important; height: 50px !important; vertical-align: middle !important;\">\r\n                            Сумма договора</th>\r\n                        <th\r\n                            style=\"width:  200px !important; height: 50px !important; vertical-align: middle !important;\">\r\n                            Срок действия договора</th>\r\n                        <th\r\n                            style=\"width:  200px !important; height: 50px !important; vertical-align: middle !important;\">\r\n                            Пролонгация</th>\r\n                        <th\r\n                            style=\"width:  200px !important; height: 50px !important; vertical-align: middle !important;\">\r\n                            Статус</th>\r\n                        <th\r\n                            style=\"width:  200px !important; height: 50px !important; vertical-align: middle !important;\">\r\n                            Скан-копия</th>\r\n                        <th\r\n                            style=\"width:  200px !important; height: 50px !important; vertical-align: middle !important;\">\r\n                            Категория</th>\r\n                        <th\r\n                            style=\"width:  200px !important; height: 50px !important; vertical-align: middle !important;\">\r\n                            Примечание</th>\r\n\r\n                        <th\r\n                            style=\"width:  200px !important; height: 50px !important; vertical-align: middle !important; background: burlywood !important;\">\r\n                            Рабочее наименование</th>\r\n                        <th\r\n                            style=\