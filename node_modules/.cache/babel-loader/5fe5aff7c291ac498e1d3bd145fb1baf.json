apper-alert';\n        this.showNotify = true;\n        setTimeout(this.closeNotification, 1500);\n      });\n    },\n\n    closeNotification() {\n      this.showNotify = false;\n    },\n\n    updateFilterDataRenivation(filter_renovation) {}\n\n  },\n  computed: { ...mapState({\n      user: state => state.auth.user,\n      uid: state => state.auth.uid,\n      allGroups: state => state.auth.groups,\n      staffGlobal: state => state.auth.users\n    })\n  }\n};","map":{"version":3,"mappings":"AA0KA;AACA;AACA;AACA;AACA;AACA;EACAA,oBADA;EAEAC;IAAAC;IAAAC;IAAAC;EAAA,CAFA;;EAGAC;IACA;MACAC,cADA;MAEAC,cAFA;MAGAC,aAHA;MAIAC,iBAJA;MAKAC,UALA;MAMAC,uBANA;MAQA;MACAC,iBATA;MAUAC,cAVA;MAWAC,iBAXA;MAYAC,eAZA;MAcAC;QACAC,UADA;QAEAC;MAFA;IAdA;EAqBA,CAzBA;;EA0BAC;IACAC;MACA;MACA;;MACA;QACA;MACA,CAFA,MAEA;QACA;MACA;;MACA;IACA,CAVA;;IAWAC;MACA;MACAC,mDACAC,IADA,CACAC;QACA;QACA;QACA;QACA;QACA;QAEA;QACA;QACA;QACA;QACAC;QACA;MACA,CAdA,EAcAC,KAdA,CAcAC;QACA;QACA;QACA;QACA;QACA;QACAF;MACA,CArBA;IAuBA,CApCA;;IAsCAG;MACA;IACA,CAxCA;;IAyCAC,+CAEA;;EA3CA,CA1BA;EAwEAC,YACA;MACAC,8BADA;MAEAC,4BAFA;MAGAC,qCAHA;MAIAC;IAJA;EADA;AAxEA","names":["name","components","Loader","Notifications","FilterRenovation","data","nextLink","prevLink","loader","total_objects","amount","renovationDirectory","showNotify","notifyHead","notifyMessage","notifyClass","filter_renovation","groups","number","methods","goToPage","getRenovationContract","api","then","response","setTimeout","catch","err","closeNotification","updateFilterDataRenivation","computed","user","uid","allGroups","staffGlobal"],"sourceRoot":"src/components/Table/Contracts","sources":["renovationContract.vue"],"sourcesContent":["<template>\r\n\r\n    <div>\r\n        <FilterRenovation @updateFilterDataRenivation=\"updateFilterDataRenivation\"></FilterRenovation>\r\n        <Notifications :show=\"showNotify\" :header=\"notifyHead\" :message=\"notifyMessage\" :block-class=\"notifyClass\"\r\n            id=\"notif\" />\r\n        <Loader :loader=\"loader\"