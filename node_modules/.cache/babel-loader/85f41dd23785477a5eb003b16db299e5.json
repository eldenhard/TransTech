{"ast":null,"code":"import { mapState } from \"vuex\";\nimport api from \"@/api/report\";\nexport default {\n  name: 'report',\n  computed: { ...mapState({\n      user: state => state.auth.user,\n      uid: state => state.auth.uid\n    })\n  },\n\n  data() {\n    return {\n      'staff': [],\n      'answer1': [],\n      'answer2': [],\n      'answer3': [],\n      'answer4': [],\n      'answer5': [],\n      'answer6': [],\n      btnName: '–°–æ–∑–¥–∞—Ç—å –æ—Ç—á–µ—Ç –ø–æ KPI —Å–æ—Ç—Ä—É–¥–Ω–∏–∫–∞',\n      'admin': [],\n      'reports_creator': [],\n      'reports_employee': []\n    };\n  },\n\n  mounted() {\n    document.getElementById('loading-page-lk').style.display = 'block'; // api.getUsers()\n\n    const pretoken = JSON.parse(localStorage.getItem(\"vuex\"));\n    const token = pretoken.auth.user.token;\n    const preid = JSON.parse(localStorage.getItem('vuex'));\n    const id = preid.auth.uid;\n    fetch('http://10.1.5.65/api/reports/kpi?' + `creator=${id}`, {\n      headers: {\n        'Authorization': `Basic ${token}`\n      },\n      method: 'GET'\n    }).then(response => {\n      if (response.ok) {\n        return response.json().then(r => {\n          this.reports_creator = r.data; // document.getElementById('loading-page-report').style.display = 'none'\n\n          console.log(this.reports_creator);\n        });\n      } else {\n        console.log('NOT OK'); // document.getElementById('loading-page-report').style.display = 'none'\n      }\n    }), fetch('http://10.1.5.65/api/reports/kpi?' + `employee=${id}`, {\n      headers: {\n        'Authorization': `Basic ${token}`\n      },\n      method: 'GET'\n    }).then(response => {\n      if (response.ok) {\n        return response.json().then(r => {\n          this.reports_employee = r.data; // document.getElementById('loading-page-report').style.display = 'none'\n\n          console.log(this.reports_employee);\n        });\n      } else {\n        console.log('NOT OK'); // document.getElementById('loading-page-report').style.display = 'none'\n      }\n    }), fetch('http://10.1.5.65/api/personal/users/?page_size=200', {\n      headers: {\n        'Authorization': `Basic ${token}`\n      },\n      method: 'GET'\n    }).then(response => {\n      if (response.ok) {\n        return response.json().then(r => {\n          this.staff = r.data;\n          document.getElementById('loading-page-lk').style.display = 'none'; // console.log(this.staff)\n        });\n      } else {\n        console.log('NOT OK');\n      }\n    }), fetch('http://10.1.5.65/api/personal/users/104', {\n      headers: {\n        'Authorization': `Basic ${token}`\n      },\n      method: 'GET'\n    }).then(response => {\n      if (response.ok) {\n        return response.json().then(r => {\n          this.admin = r; // document.getElementById('loading-page-lk').style.display = 'none'\n          // console.log(this.admin)\n        });\n      } else {\n        console.log('NOT OK');\n      }\n    }), fetch('http://10.1.5.65/api/reports/kpi/', {\n      headers: {\n        'Authorization': `Basic ${token}`\n      },\n      method: 'GET'\n    }).then(response => {\n      if (response.ok) {\n        return response.json().then(r => {\n          this.all_reports = r.data;\n          document.getElementById('loading-page-report').style.display = 'none';\n          console.log(this.all_reports);\n        });\n      } else {\n        console.log('NOT OK');\n        document.getElementById('loading-page-report').style.display = 'none';\n      }\n    });\n  },\n\n  methods: {\n    Not() {\n      document.getElementById('notifications').style.display = 'none';\n    },\n\n    Not2() {\n      document.getElementById('notifications-2').style.display = 'none';\n    },\n\n    OpenKPI() {\n      if (document.getElementById(\"Anketa\").style.display == 'block') {\n        document.getElementById(\"Anketa\").style.display = \"none\";\n        this.btnName = '–°–æ–∑–¥–∞—Ç—å –æ—Ç—á–µ—Ç –ø–æ KPI —Å–æ—Ç—Ä—É–¥–Ω–∏–∫–∞';\n      } else {\n        document.getElementById(\"Anketa\").style.display = \"block\";\n        this.btnName = '–°–∫—Ä—ã—Ç—å –æ—Ç—á–µ—Ç –ø–æ KPI —Å–æ—Ç—Ä—É–¥–Ω–∏–∫–∞';\n      }\n    },\n\n    OpenReport() {\n      if (document.getElementById(\"tables\").style.display == 'block') {\n        document.getElementById(\"tables\").style.display = \"none\";\n      } else {\n        document.getElementById(\"tables\").style.display = \"block\";\n      }\n    },\n\n    Send() {\n      event.preventDefault();\n      const pretoken = JSON.parse(localStorage.getItem(\"vuex\"));\n      const token = pretoken.auth.user.token;\n      document.getElementById('loading-page-lk').style.display = 'block';\n      let admin = document.getElementById('admin').value.split(' ')[0];\n      let staff = document.getElementById('staff').value.split(' ')[0];\n      fetch('http://10.1.5.65/api/reports/kpi/create/', {\n        method: 'POST',\n        headers: {\n          'Content-Type': 'application/json;charset=utf-8',\n          'Authorization': `Basic ${token}`\n        },\n        body: JSON.stringify({\n          \"employee\": staff,\n          \"manager\": admin,\n          \"a1\": this.answer1,\n          \"a2\": this.answer2,\n          \"a3\": this.answer3,\n          \"a4\": this.answer4,\n          \"a5\": this.answer5,\n          \"a6\": this.answer6\n        })\n      }).then(response => {\n        if (response.ok) {\n          return response.json().then(data => {\n            console.log(data);\n            document.getElementById('loading-page-lk').style.display = 'none';\n            document.getElementById('notifications').style.display = 'block';\n            setTimeout(this.Not, 1500);\n            document.getElementById(\"Anketa\").reset();\n          });\n        } else {\n          document.getElementById('loading-page-lk').style.display = 'none';\n          document.getElementById('notifications-2').style.display = 'block';\n          setInterval(this.Not2, 1500);\n          console.log(response);\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoaA;AACA;AAEA;EACAA,cADA;EAEAC,YACA;MACAC,8BADA;MAEAC;IAFA;EADA,CAFA;;EAQAC;IACA;MACA,WADA;MAEA,aAFA;MAGA,aAHA;MAIA,aAJA;MAKA,aALA;MAMA,aANA;MAOA,aAPA;MAQAC,0CARA;MASA,WATA;MAUA,qBAVA;MAWA;IAXA;EAcA,CAvBA;;EAwBAC;IACAC,mEADA,CAEA;;IACA;IACA;IACA;IACA;IACAC;MACAC;QACA;MADA,CADA;MAIAC;IAJA,GAMAC,IANA,CAMAC;MACA;QACA;UACA,8BADA,CAEA;;UACAC;QACA,CAJA;MAKA,CANA,MAOA;QACAA,sBADA,CAEA;MAEA;IACA,CAnBA,GAoBAL;MACAC;QACA;MADA,CADA;MAIAC;IAJA,GAMAC,IANA,CAMAC;MACA;QACA;UACA,+BADA,CAEA;;UACAC;QACA,CAJA;MAKA,CANA,MAOA;QACAA,sBADA,CAEA;MAEA;IACA,CAnBA,CApBA,EAwCAL;MACAC;QACA;MADA,CADA;MAIAC;IAJA,GAMAC,IANA,CAMAC;MACA;QACA;UACA;UACAL,kEAFA,CAGA;QACA,CAJA;MAKA,CANA,MAOA;QACAM;MACA;IACA,CAjBA,CAxCA,EA0DAL;MACAC;QACA;MADA,CADA;MAIAC;IAJA,GAMAC,IANA,CAMAC;MACA;QACA;UACA,eADA,CAEA;UACA;QACA,CAJA;MAKA,CANA,MAOA;QACAC;MACA;IACA,CAjBA,CA1DA,EA4EAL;MACAC;QACA;MADA,CADA;MAIAC;IAJA,GAMAC,IANA,CAMAC;MACA;QACA;UACA;UACAL;UACAM;QACA,CAJA;MAKA,CANA,MAOA;QACAA;QACAN;MAEA;IACA,CAnBA,CA5EA;EAgGA,CA/HA;;EAgIAO;IACAC;MACAR;IACA,CAHA;;IAIAS;MACAT;IACA,CANA;;IAQAU;MACA;QACAV;QACA;MACA,CAHA,MAIA;QACAA;QACA;MACA;IACA,CAjBA;;IAmBAW;MACA;QACAX;MACA,CAFA,MAGA;QACAA;MACA;IAEA,CA3BA;;IA6BAY;MACAC;MACA;MACA;MACAb;MACA;MACA;MACAC;QACAE,cADA;QAEAD;UACA,gDADA;UAEA;QAFA,CAFA;QAMAY;UACA,iBADA;UAEA,gBAFA;UAGA,kBAHA;UAIA,kBAJA;UAKA,kBALA;UAMA,kBANA;UAOA,kBAPA;UAQA;QARA;MANA,GAiBAV,IAjBA,CAiBAC;QACA;UACA;YACAC;YACAN;YACAA;YACAe;YACAf;UACA,CANA;QAOA,CARA,MASA;UACAA;UACAA;UACAgB;UACAV;QAEA;MACA,CAlCA;IAmCA;;EAvEA;AAhIA","names":["name","computed","user","uid","data","btnName","mounted","document","fetch","headers","method","then","response","console","methods","Not","Not2","OpenKPI","OpenReport","Send","event","body","setTimeout","setInterval"],"sourceRoot":"src/views","sources":["report.vue"],"sourcesContent":["<template>\r\n    <div class=\"lk\">\r\n        <h2>–û—Ç—á–µ—Ç—ã</h2>\r\n   \r\n        <button class=\"button Action\" @click=\"OpenKPI()\" style=\"width: 40%; position: relative; left: 50%; transform: translate(-50%,0); font-size: 17px; margin-top: 3%\">{{ btnName }}</button>\r\n        <br><br>\r\n    <form id=\"Anketa\" method=\"post\" style=\"display: none\">\r\n        <br>\r\n    <label  for=\"admin\" style=\" position: relative; left: 50%; transform: translate(-50%, 0); width: 100% !important;\">\r\n\r\n    <p style=\"text-align:center;\">–ö–æ–ø–∏—è –ø–∏—Å—å–º–∞: –∫–∞–¥—Ä–æ–≤–∞—è —Å–ª—É–∂–±–∞ (–æ–±—è–∑–∞—Ç–µ–ª—å–Ω–æ–µ –ø–æ–ª–µ)</p>    \r\n    <select id=\"admin\" required style=\"width: 50% !important;\" >\r\n        <option selected=\"selected\">{{admin.id}} {{admin.first_name}} {{admin.last_name}}</option>\r\n    </select>\r\n    </label>\r\n\r\n<br>\r\n<br>\r\n\r\n    <label  for=\"staff\" style=\" position: relative; left: 50%; transform: translate(-50%, 0); width: 100% !important;\">\r\n        \r\n        <select id=\"staff\" required style=\"width: 50% !important;\">\r\n            <option value=\"\" disabled=\"disabled\" selected=\"selected\">–°–æ—Ç—Ä—É–¥–Ω–∏–∫</option>\r\n            <option  v-for=\"staf in staff\" :key=\"staf.id\">{{staf.id}} {{staf.first_name}} {{staf.last_name}}</option>\r\n        </select>\r\n    </label>\r\n\r\n\r\n\r\n\r\n        <div id=\"block-answer\">\r\n            <h2>–ê–Ω–∫–µ—Ç–∞ —Å–æ—Ç—Ä—É–¥–Ω–∏–∫–∞</h2>\r\n      <p class=\"answer\" name=\"a1\">–ì–æ—Ç–æ–≤–Ω–æ—Å—Ç—å –Ω–µ—É–∫–æ—Å–Ω–∏—Ç–µ–ª—å–Ω–æ –≤—ã–ø–æ–ª–Ω—è—Ç—å –≤—Å–µ –ø—Ä–æ–∏–∑–≤–æ–¥—Å—Ç–≤–µ–Ω–Ω—ã–µ –∑–∞–¥–∞–Ω–∏—è, –ø–æ—Ä—É—á–µ–Ω–Ω—ã–µ —Ä—É–∫–æ–≤–æ–¥–∏—Ç–µ–ª–µ–º.</p>\r\n\r\n     \r\n        <input type=\"radio\" name=\"first-question\" id=\"first-question-1\"  value=\"c1\" v-model=\"answer1\">\r\n        <label for=\"first-question-1\">&nbsp;–ù–µ–∏—Å–ø–æ–ª–Ω–∏—Ç–µ–ª–µ–Ω, —Å–∫–ª–æ–Ω–µ–Ω –ø–æ–¥ –ª—é–±—ã–º–∏ –ø—Ä–µ–¥–ª–æ–≥–∞–º–∏ –∏–∑–±–µ–≥–∞—Ç—å –ø–æ–ª—É—á–µ–Ω–∏—è –Ω–æ–≤—ã—Ö –∑–∞–¥–∞–Ω–∏–π. –ë—ã–ª–∏ —Å–ª—É—á–∞–∏ –ø—Ä—è–º–æ–≥–æ –æ—Ç–∫–∞–∑–∞ –æ—Ç –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è –∑–∞–¥–∞–Ω–∏–π.</label><br>\r\n\r\n        <input type=\"radio\" name=\"first-question\" id=\"first-question-2\" value=\"c2\"  v-model=\"answer1\">\r\n        <label  for=\"first-question-2\">&nbsp;–ù–∏–∑–∫–∏–π —É—Ä–æ–≤–µ–Ω—å –∏—Å–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏. –ò–Ω–æ–≥–¥–∞ –ø—ã—Ç–∞–µ—Ç—Å—è –∏–∑–±–µ–∂–∞—Ç—å –ø–æ–ª—É—á–µ–Ω–∏—è –Ω–æ–≤—ã—Ö –∑–∞–¥–∞–Ω–∏–π, —É–º–µ–ª–æ –Ω–∞—Ö–æ–¥—è –ø—Ä–µ–¥–ª–æ–≥–∏</label><br>\r\n\r\n        <input type=\"radio\" name=\"first-question\" id=\"first-question-3\"  value=\"c3\"  v-model=\"answer1\">\r\n        <label for=\"first-question-3\" >&nbsp;–•–æ—Ä–æ—à–∏–π —É—Ä–æ–≤–µ–Ω—å –∏—Å–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏, –Ω–æ –Ω–µ–ª—å–∑—è —Å–∫–∞–∑–∞—Ç—å, —á—Ç–æ –∫–∞–∂–¥—ã–π –¥–µ–Ω—å –±–µ–∑ –∏—Å–∫–ª—é—á–µ–Ω–∏–π. –ë–µ—Ä–µ—Ç—Å—è –∑–∞ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –ª—é–±—ã—Ö –ø—Ä–æ–∏–∑–≤–æ–¥—Å—Ç–≤–µ–Ω–Ω—ã—Ö –∑–∞–¥–∞–Ω–∏–π, –Ω–æ –Ω–µ –≤—Å–µ–≥–¥–∞ –æ—Ö–æ—Ç–Ω–æ.</label><br>\r\n\r\n        <input type=\"radio\" name=\"first-question\" id=\"first-question-4\"  value=\"c4\"  v-model=\"answer1\">\r\n        <label for=\"first-question-4\" >&nbsp;–ë–µ–∑—É–∫–æ—Ä–∏–∑–Ω–µ–Ω–Ω—ã–π —É—Ä–æ–≤–µ–Ω—å –∏—Å–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏. –í—Å–µ–≥–¥–∞ –æ—Ö–æ—Ç–Ω–æ –±–µ—Ä–µ—Ç—Å—è –∑–∞ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –≤—Å–µ—Ö –ø—Ä–æ–∏–∑–≤–æ–¥—Å—Ç–≤–µ–Ω–Ω—ã—Ö –∑–∞–¥–∞–Ω–∏–π, –ø–æ—Ä—É—á–µ–Ω–Ω—ã—Ö —Ä—É–∫–æ–≤–æ–¥–∏—Ç–µ–ª–µ–º.</label><br>\r\n<hr>\r\n       <p class=\"answer\"  name=\"a2\">–°–ø–æ—Å–æ–±–Ω–æ—Å—Ç—å —Å–ø—Ä–∞–≤–ª—è—Ç—å—Å—è —Å–æ —Å–≤–æ–∏–º–∏ –æ–±—è–∑–∞–Ω–Ω–æ—Å—Ç—è–º–∏ –∏ –ø–æ—Ä—É—á–µ–Ω–∏—è–º–∏. –£–º–µ–Ω–∏–µ –≤—ã—è–≤–ª—è—Ç—å –∏ —Ä–µ—à–∞—Ç—å –≤–æ–∑–Ω–∏–∫–∞—é—â–∏–µ –≤ —Ä–∞–±–æ—Ç–µ –ø—Ä–æ–±–ª–µ–º—ã</p>\r\n     \r\n        <input type=\"radio\" name=\"second-question\" id=\"second-question-1\"  value=\"c1\"  v-model=\"answer2\">\r\n        <label for=\"second-question-1\" >&nbsp;–ß–∞—Å—Ç–æ –Ω–µ —Å–ø—Ä–∞–≤–ª—è–µ—Ç—Å—è —Å–æ —Å–≤–æ–∏–º–∏ –æ–±—è–∑–∞–Ω–Ω–æ—Å—Ç—è–º–∏ –∏ –ø–æ—Ä—É—á–µ–Ω–∏—è–º–∏ –∏–ª–∏ –∑–∞–¥–∞–Ω–∏—è–º–∏. –ù–µ —É–º–µ–µ—Ç –≤—ã—è–≤–ª—è—Ç—å –ø—Ä–æ–±–ª–µ–º—ã –∏ —Å —Ç—Ä—É–¥–æ–º —Ä–µ—à–∞–µ—Ç –∏—Ö</label><br>\r\n\r\n        <input type=\"radio\" name=\"second-question\" id=\"second-question-2\"  value=\"c2\"  v-model=\"answer2\">\r\n        <label for=\"second-question-2\">&nbsp;–ö–∞–∫ –ø—Ä–∞–≤–∏–ª–æ —Å–ø—Ä–∞–≤–ª—è–µ—Ç—Å—è —Å–æ —Å–≤–æ–∏–º–∏ –æ–±—è–∑–∞–Ω–Ω–æ—Å—Ç—è–º–∏, –Ω–æ –ø–æ—Ä–æ–π –Ω–µ –≤—ã–ø–æ–ª–Ω—è–µ—Ç –∏—Ö —Ç–∞–∫ –∫–∞–∫ –Ω–∞–¥–æ –∏–ª–∏ —Å —Ç—Ä—É–¥–æ–º —Ä–µ—à–∞–µ—Ç –≤–æ–∑–Ω–∏–∫–∞—é—â–∏–µ –ø—Ä–æ–±–ª–µ–º—ã</label><br>\r\n\r\n        <input type=\"radio\" name=\"second-question\" id=\"second-question-3\"  value=\"c3\"  v-model=\"answer2\">\r\n        <label for=\"second-question-3\" > &nbsp;–ü—Ä–∞–∫—Ç–∏—á–µ—Å–∫–∏ –≤—Å–µ–≥–¥–∞ —Ö–æ—Ä–æ—à–æ —Å–ø—Ä–∞–≤–ª—è–µ—Ç—Å—è —Å–æ –≤—Å–µ–º–∏ –æ–±—è–∑–∞–Ω–Ω–æ—Å—Ç—è–º–∏, —É–º–µ–ª–æ —Ä–µ—à–∞–µ—Ç –ø—Ä–æ–±–ª–µ–º—ã, –Ω–æ –Ω–µ–ª—å–∑—è —Å–∫–∞–∑–∞—Ç—å, —á—Ç–æ –∫–∞–∂–¥—ã–π –¥–µ–Ω—å –±–µ–∑ –∏—Å–∫–ª—é—á–µ–Ω–∏–π</label><br>\r\n\r\n        <input type=\"radio\" name=\"second-question\" id=\"second-question-4\"  value=\"c4\"  v-model=\"answer2\">\r\n        <label for=\"second-question-4\" > &nbsp;–ü—Ä–æ—è–≤–ª—è–µ—Ç –æ—Ç–ª–∏—á–Ω—É—é —Å–ø–æ—Å–æ–±–Ω–æ—Å—Ç—å —Å–ø—Ä–∞–≤–ª—è—Ç—å—Å—è —Å–æ –≤—Å–µ–º–∏ –æ–±—è–∑–∞–Ω–Ω–æ—Å—Ç—è–º–∏ –∏ –ø—Ä–æ–∏–∑–≤–æ–¥—Å—Ç–≤–µ–Ω–Ω—ã–º–∏ –∑–∞–¥–∞–Ω–∏—è–º–∏. –í—Å–µ–≥–¥–∞ —É–º–µ–ª–æ –≤—ã—è–≤–ª—è–µ—Ç –∏ –±—ã—Å—Ç—Ä–æ —Ä–µ—à–∞–µ—Ç –≤–æ–∑–Ω–∏–∫–∞—é—â–∏–µ –ø—Ä–æ–±–ª–µ–º—ã</label><br>\r\n     \r\n        <hr>\r\n\r\n    <p class=\"answer\"  name=\"a3\">–ö–æ–º–ø–µ—Ç–µ–Ω—Ç–Ω–æ—Å—Ç—å. –ó–Ω–∞–Ω–∏–µ –∏—Å–ø–æ–ª—å–∑—É–µ–º—ã—Ö –ø—Ä–∏–µ–º–æ–≤ –∏ –º–µ—Ç–æ–¥–æ–≤ —Ä–∞–±–æ—Ç—ã –∏ —É–º–µ–Ω–∏–µ –≤ —Ç–æ—á–Ω–æ—Å—Ç–∏ –∏–º —Å–ª–µ–¥–æ–≤–∞—Ç—å, –Ω–∞–ª–∏—á–∏–µ –Ω–µ–æ–±—Ö–æ–¥–∏–º—ã—Ö –Ω–∞–≤—ã–∫–æ–≤</p>\r\n\r\n     \r\n        <input type=\"radio\" name=\"third-question\" id=\"third-question-1\"   value=\"c1\"  v-model=\"answer3\">\r\n        <label for=\"third-question-1\">&nbsp;–ù–µ–∫–æ–º–ø–µ—Ç–µ–Ω—Ç–µ–Ω. –ù–µ –∑–Ω–∞–µ—Ç –∏—Å–ø–æ–ª—å–∑—É–µ–º—ã–µ –ø—Ä–∏–µ–º—ã –∏ –º–µ—Ç–æ–¥—ã —Ä–∞–±–æ—Ç—ã –∏ –Ω–µ —Å—Ç—Ä–µ–º–∏—Ç—Å—è –±—ã—Å—Ç—Ä–æ –æ—Å–≤–æ–∏—Ç—å –∏—Ö</label><br>\r\n\r\n     