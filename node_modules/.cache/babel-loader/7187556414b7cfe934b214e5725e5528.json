}\r\n</style>\r\n\r\n\r\n<script>\r\nimport {mapState} from \"vuex\";\r\nexport default{\r\nname: 'report',\r\ndata(){\r\n    return{\r\n        'staff': [],\r\n        'answer1': [],\r\n        'answer2': [],\r\n        'answer3': [],\r\n        'answer4': [],\r\n        'answer5': [],\r\n        'answer6': []\r\n\r\n    }\r\n},\r\nmounted(){\r\n    document.getElementById('loading-page-lk').style.display = 'block'\r\n    const pretoken = JSON.parse(localStorage.getItem(\"vuex\"))\r\n    const token = pretoken.user.token\r\n            fetch('http://10.1.5.65/api/personal/users/', {\r\n        headers: {\r\n            'Authorization': `Basic ${token}` \r\n        },\r\n        method: 'GET'\r\n    })\r\n    .then((response) => {\r\n                if (response.ok){\r\n                    return response.json().then(r=>{\r\n                        this.staff = r.data;\r\n                        document.getElementById('loading-page-lk').style.display = 'none'\r\n                        console.log(this.staff)\r\n                })\r\n            }\r\n            else{\r\n                console.log('NOT OK')\r\n            }\r\n        })\r\n},\r\nmethods: { \r\n OpenKPI(){\r\n    if (document.getElementById(\"Anketa\").style.display == 'block') { \r\n        document.getElementById(\"Anketa\").style.display = \"none\";\r\n    }\r\n    else {\r\n        document.getElementById(\"Anketa\").style.display = \"block\";\r\n        }\r\n },    \r\n Send(){\r\n    event.preventDefault()\r\n    const pretoken = JSON.parse(localStorage.getItem(\"vuex\"))\r\n    const token = pretoken.user.token\r\n    document.getElementById('loading-page-lk').style.display = 'block'\r\n   let admin = document.getElementById('admin').value.split(' ')[0] \r\n   let staff = document.getElementById('staff').value.split(' ')[0] \r\n   fetch('http://10.1.5.65/api/reports/kpi/create/', {\r\n    method: 'POST',\r\n    headers: {\r\n        'Content-Type': 'application/json;charset=utf-8',\r\n        'Authorization': `Basic ${token}` \r\n    },\r\n    body: JSON.stringify({\r\n    \"employee\": staff,\r\n    \"manager\": admin,\r\n    \"a1\": this.answer1,\r\n    \"a2\": this.answer2,\r\n    \"a3\": this.answer3,\r\n    \"a4\": this.answer4,\r\n    \"a5\": this.answer5,\r\n    \"a6\": this.answer6\r\n })\r\n})\r\n\r\n\r\n\r\n  .then((response) => \r\n    {\r\n        if (response.ok){\r\n            return response.json().then((data)=>{\r\n                console.log(data);\r\n                document.getElementById('loading-page-lk').style.display = 'none';\r\n                alert('–ê–Ω–∫–µ—Ç–∞ –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–∞')\r\n\r\n            })\r\n           \r\n        }\r\n        else{\r\n            document.getElementById('loading-page-lk').style.display = 'none';\r\n            alert('–û—à–∏–±–∫–∞, –ø–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â—ë —Ä–∞–∑')\r\n            console.log(response)\r\n\r\n        }\r\n    })\r\n }\r\n},\r\ncomputed: {\r\n    ...mapState({\r\n      user: state => state.user\r\n    })\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n\r\n    "]},"metadata":{},"sourceType":"module"}